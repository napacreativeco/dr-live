<div class="thumbnail-rack media">

    {% for m in product.media %}
    
        {% if m != null %}
            <div class="media-thumbnail" data-id="{{ v.id }}" data-image="{{ m | image_url }}">
                <img src="{{ m | image_url: width: '500' }}" />
            </div>
        {% endif %}

    {% endfor %}

</div>